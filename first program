a=input('Enter the first number: ')
b=input('Enter the second number: ')
a=int(a)
b=int(b)
A=a+b
S=a-b
M=a*b
D=a/b
print('Addition: ',A)
print('Subtraction: ',S)
print('Multiplication: ',M)
print('Division: ',D)



